{
  "name": "blue-teapot-scheduling",
  "version": "2.3.19",
  "private": true,
  "homepage": "./",
  "author": "Conor Scott",
  "license": "UNLICENSED",
  "description": "Desktop and web app for Blue Teapot scheduling",
  "main": "electron-main.js",
  "build": {
    "appId": "ie.blueteapot.timetable",
    "productName": "Blue Teapot Schedule",
    "directories": {
      "output": "dist",
      "buildResources": "buildResources"
    },
    "asar": true,
    "files": [
      "build/**/*",
      "electron-main.js"
    ],
    "extraMetadata": {
      "main": "electron-main.js"
    },
    "mac": {
      "icon": "buildResources/icon.icns",
      "target": [
        "dmg"
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "buildResources/entitlements.mac.plist",
      "entitlementsInherit": "buildResources/entitlements.mac.plist",
      "notarize": {
    "teamId": "ptzh-obzy-bxmh-sqjg"
  }
    },
    "win": {
      "icon": "buildResources/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "icon": "buildResources/icon.png"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "ConScottDev",
        "repo": "blueteapot-timetable-app"
      }
    ]
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/push-notifications": "^7.0.4",
    "@date-io/dayjs": "^3.0.0",
    "@emotion/cache": "11.10.8",
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.12.3",
    "@mui/x-date-pickers": "^7.13.0",
    "chart.js": "4.3.0",
    "chroma-js": "2.4.2",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.12",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "firebase": "^10.12.4",
    "firebase-admin": "^13.5.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-big-calendar": "^1.13.1",
    "react-chartjs-2": "5.2.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^7.3.0",
    "react-dom": "18.2.0",
    "react-github-btn": "1.4.0",
    "react-modal": "^3.16.1",
    "react-multi-date-picker": "^4.5.2",
    "react-router-dom": "6.11.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.8.0",
    "react-table": "7.8.0",
    "react-to-print": "^2.15.1",
    "styled-components": "^6.1.12",
    "stylis": "4.1.4",
    "stylis-plugin-rtl": "2.1.1",
    "yup": "1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rd /s /q node_modules && del package-lock.json && npm install && npm start",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "dev": "concurrently \"npm run react:start\" \"npm run electron:dev\"",
    "build:desktop:local": "npm run react:build && electron-builder --publish=never",
    "build:desktop:publish": "cross-env CI=false npm run react:build && electron-builder --publish always",
    "ios:sync": "npm run build && npx cap sync ios",
    "ios:open": "npx cap open ios"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/prop-types": "off",
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "ajv": "^7.2.4",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "2.8.8"
  },
  "overrides": {
    "svgo": "3.0.2"
  }
}
